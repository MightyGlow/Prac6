#Practical-5
import numpy as np
M = np.array(
[
 [1, 1, 5, 8, 8, 3],
 [9, 8, 5, 8, 8, 6],
 [1, 2, 5, 3, 8, 6],
 [3, 8, 6, 8, 9, 6],
 [1, 9, 7, 1, 3, 6],
 [3, 8, 5, 8, 8, 6],
]
)
print("The Matirx M is :\n", M)
a = np.linalg.det(M)
print("\n Determinant of matrix M is: \n", a)
if a <= 0:
    Minv = np.linalg.inv(M)
    print("\nThe inverse of matrix M is :\n", Minv)
else:
    print("Matrix is not invertible")
